{
	"name": "roblox-ts-game",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"$path": "out/server"
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$path": "node_modules/@rbxts",
					"@flamework": {
						"$path": "node_modules/@flamework"
					}
				}
			},

			"$path": "out/shared",

			"robotModel" : {
				"$path": "assets/robotModel.rbxm"
			},

			"box": {
				"$path": "assets/box.rbxm"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"$path": "out/client"
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			},

			"Map": {
				"$path": "assets/map.rbxm"
			},

			"Terrain": {
				"$path": "assets/terrain.rbxm"
			},

			"robots": {
				"$className": "Folder"
			},

			"characters": {
				"$className": "Folder"
			},

			"debug": {
				"$className": "Folder"
			},

			"notes": {
				"$path": "assets/notes.rbxm"
			},

			"boxPiles": {
				"$className": "Folder",

				"Pile1": {
					"$path": "assets/pile1.rbxm"
				},

				"Pile2": {
					"$path": "assets/pile2.rbxm"
				}
			}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		},
		"Lighting": {
			"$className": "Lighting",
			"$properties": {
				"Ambient": [0.27, 0.27, 0.27],
				"Brightness": 3,
				"EnvironmentDiffuseScale": 1,
				"EnvironmentSpecularScale": 1,
				"GlobalShadows": true,
				"OutdoorAmbient": [0.27, 0.27, 0.27],
				"ShadowSoftness": 0.2,
				"ClockTime": 14.5,
				"GeographicLatitude": 0
			}
		}
	}
}
